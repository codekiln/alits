{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "dangerouslyDisablePackageManagerCheck": true,
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        "Project/**",
        "Code/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false,
      "persistent": true
    },
    "maxmsp": {
      "cache": false,
      "persistent": false
    },
    "release": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": false
    },
    "clean": {
      "cache": false,
      "persistent": false
    }
  }
}

{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES5",
    "ignoreDeprecations": "5.0",
    "strict": false,
    "noImplicitAny": false,
    "sourceMap": false,
    "outDir": "./fixtures",
    "baseUrl": ".",
    "types": [],
    "typeRoots": ["../../../../node_modules/@types", "../../../node_modules/@types"],
    "paths": {
      "@alits/core": ["../../../dist/index.d.ts"]

{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "es2015.promise"],
    "module": "commonjs",
    "outDir": "./fixtures",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "fixtures", "results"]
}

{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES5",
    "ignoreDeprecations": "5.0",
    "strict": true,
    "noImplicitAny": true,
    "sourceMap": false,
    "outDir": "./Project",
    "baseUrl": "src",
    "types": ["maxmsp"],
    "lib": ["es5"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules", "../../node_modules"]
} 